<view><cu-custom vue-id="bae1c640-1" bgColor="bg-white" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">用户中心</view></cu-custom><view class="cu-card   bg-white margin-top padding userinfo"><view class="cu-avatar radius  xl head"></view><view class="info"><view class="username text-lg  padding-tb-xs">{{userInfo.RealName}}</view><view class="text-gray">{{userInfo.Type=="T"?"(教师)":userInfo.Type=="S"?"(学生)":""}}</view></view><view class="idcode text-gray text-sm">{{''+(userInfo.Type=="T"?"职工号：":userInfo.Type=="S"?"学号：":"")+userInfo.IdentityCode+''}}</view></view><view class="cu-list  bg-white  menu margin-top "><block wx:if="{{userInfo.Type}}"><view data-event-opts="{{[['tap',[['$url',['/pages/term/term']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-calendar text-orange"></text><text class="text-grey">学校校历</text></view></view></block><block wx:if="{{userInfo.Type}}"><view data-event-opts="{{[['tap',[['$url',['/pages/mycourse/mycourse']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-read text-yellow"></text><text class="text-grey">我的课程</text></view></view></block></view><view class="cu-list  bg-white  menu margin-top "><block wx:if="{{userInfo.Type}}"><view data-event-opts="{{[['tap',[['$url',['/pages/password/password']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-lock text-green"></text><text class="text-grey">修改密码</text></view></view></block><block wx:if="{{userInfo.Type}}"><view data-event-opts="{{[['tap',[['$url',['/pages/login/login']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-exit text-red"></text><text class="text-grey">重新登录</text><view class="tagtip">若登录的账号不正确</view></view></view></block></view><block wx:if="{{userInfo.Type}}"><button data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="cu-btn bg-red margin block lg" bindtap="__e">解除绑定并退出登陆</button></block></view>